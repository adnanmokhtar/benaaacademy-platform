{
    "name": "dot/platform",
    "description": "Just a laravel plugins platform",
    "keywords": [
        "framework",
        "laravel",
        "cms",
        "plugins",
        "platform",
        "dot"
    ],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-bcmath": "*",
        "laravel/framework": "*",
        "barryvdh/laravel-debugbar": "^3.9",
        "doctrine/dbal": "^3.8",
        "laravel/helpers": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Platform\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dot\\Platform\\ServiceProvider"
            ]
        }
    }
}
